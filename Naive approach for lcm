#include<iostream>
using namespace std;
int lcm(int a, int b){
int res = max(a,b);
while(true){
if(res%a == 0 && res %b == 0){
return res;
}
res++;
}
return res;
}
int main(){
cout<<"enter the two numbers whose LCM you need"<<endl;
int a,b;
cin>>a>>b;
cout<<"the lcm of the two numbers are"<<lcm(a,b)<<endl;
return 0;
}
